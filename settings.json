  "combat-tracker-dock": {
    "overflowStyle": "scroll",
    "portraitSize": "70px"
  },
  "uiConfig": {
    "fadeOpacity": 0.9,
    "chatNotifications": "pip"
  },
  "keybindings": {
    "dnd5e.dragCopy": [],
    "dnd5e.skipDialogNormal": [
      { "key": "AltLeft", "modifiers": ["Control"] }
    ],
    "dnd5e.skipDialogAdvantage": [
      { "key": "ShiftLeft", "modifiers": [] },
      { "key": "ShiftRight", "modifiers": [] }
